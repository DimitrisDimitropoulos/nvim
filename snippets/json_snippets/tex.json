{
  "BibLaTex-Setup": {
    "prefix": "biblatex-setup",
    "body": [
      "\\usepackage[backend=biber,language=auto,autolang=other,style=ieee,sorting=nyt,defernumbers=true,doi=false]{biblatex}",
      "\\addbibresource{$1}\n$0"
    ],
    "description": "BibLaTeX setup"
  },
  "Code": {
    "prefix": "code",
    "body": [
      "\\usepackage{listings}",
      "\\usepackage{verbatim}",
      "\\usepackage{algpseudocode}",
      "\\lstset{",
      "\tlanguage= ${1|Matlab,C++,C,Python,Julia|},",
      "\tbasicstyle=\\ttfamily,",
      "\tkeywordstyle=\\color{blue},",
      "\tcommentstyle=\\color{green!50!black},",
      "\tstringstyle=\\color{red},",
      "\tshowstringspaces=false,",
      "\tfirstnumber=1,",
      "\tnumbers=left,",
      "\tstepnumber=1,",
      "\tbreaklines=true,",
      "}\n$0"
    ],
    "description": "LaTeX code setup"
  },
  "Inkscape": {
    "prefix": "inkscape",
    "body": [
      "\\usepackage{import}",
      "\\usepackage{xifthen}",
      "\\usepackage{pdfpages}",
      "\\usepackage{transparent}",
      "\\usepackage{svg}",
      "\\newcommand{\\incfig}[1]{%",
      "\t\\def\\svgwidth{0.5\\textwidth}",
      "\t\\import{./figures/}{#1.pdf_tex}",
      "}",
      "\\newcommand{\\incFig}[1]{%",
      "\t\\def\\svgwidth{0.5\\textwidth}",
      "\t\\import{./figures/}{#1}",
      "}\n$0"
    ],
    "description": "LaTeX Inkscape"
  },
  "hyperref": {
    "prefix": "hyperref",
    "body": [
      "\\usepackage{hyperref}",
      "\\hypersetup{",
      "\t colorlinks=true,",
      "\t linktoc=all,",
      "\t citecolor=black,",
      "\t filecolor=black,",
      "\t linkcolor=black,",
      "\t urlcolor=black",
      "}",
      "\\hypersetup{linktocpage}\n$0"
    ],
    "description": "LaTeX hyperref-setup"
  },
  "LaTeX-tikz-setup": {
    "prefix": "tikz-setup",
    "body": [
      "\\usepackage{tikz}",
      "\\usepackage{pgfplots}",
      "\\pgfplotsset{compat=1.18}\n$0"
    ],
    "description": "LaTeX tikz setup"
  },
  "environment": {
    "prefix": "envu",
    "body": [
      "\\begin{$1}",
      "\t$0",
      "\\end{$1}\n"
    ],
    "description": "LaTeX environment"
  },
  "LaTeX-tikz": {
    "prefix": "tikzu",
    "body": [
      "\\begin{figure}[ht]",
      "\t\\centering",
      "\t\\begin{tikzpicture}",
      "\t\t$0",
      "\t\\end{tikzpicture}",
      "\t\\caption{$1}\\label{fig:$2}",
      "\\end{figure}\n"
    ],
    "description": "LaTeX tikz"
  },
  "LaTeX-Table": {
    "prefix": "tabu",
    "body": [
      "",
      "\\begin{table}[ht]",
      "\t\\centering",
      "\t\\caption{$1}\\label{tab:$2}",
      "\t\\begin{tabular}[H]{ccc}",
      "\t\t\\\\toprule",
      "\t\t$0 & &",
      "\t\t\\\\\\\\  \\midrule \\midrule",
      "\t\t&  & ",
      "\t\t\\\\\\\\ \\midrule",
      "\t\t&  & ",
      "\t\t\\\\\\\\ \\bottomrule",
      "\t\t\\end{tabular}",
      "\\end{table}\n"
    ],
    "description": "LaTeX table"
  },
  "LaTeX-Tabularx": {
    "prefix": "tabux",
    "body": [
      "\\begin{table}[ht]",
      "\t\\centering",
      "\t\\caption{$1}\\label{tab:$2}",
      "\t\\begin{tabularx}{\\textwidth}{XXX}",
      "\t\t\\toprule",
      "\t\t$0 & &",
      "\t\t\\\\\\\\  \\midrule \\midrule",
      "\t\t&  & ",
      "\t\t\\\\\\\\ \\midrule",
      "\t\t&  & ",
      "\t\t\\\\\\\\ \\bottomrule",
      "\t\\end{tabularx}",
      "\\end{table}\n"
    ],
    "description": "LaTeX tabularx"
  },
  "LaTeX-Figure": {
    "prefix": [
      "figu",
      "diagu"
    ],
    "body": [
      "\\begin{figure}[ht]",
      "\t\\centering",
      "\t\\includegraphics[width = \\textwidth]{$1}",
      "\t\\caption{$2}\\label{fig:$0}",
      "\\end{figure}\n"
    ],
    "description": "LaTeX figure"
  },
  "BibTeX": {
    "prefix": "bib-biblio",
    "body": [
      "\\newpage",
      "\\section{Βιβλιογρφία}",
      "\\bibliography{${1:file}}",
      "\\bibliographystyle{${0:style_eg_acm}}\n"
    ],
    "description": "BibTeX bibliography"
  },
  "BibLaTeX": {
    "prefix": "bibu",
    "body": [
      "\\newpage",
      "\\section{Βιβλιογρφία}",
      "\\nocite{*}",
      "\\printbibliography[heading=none]\n$0"
    ],
    "description": "BibLaTeX bibliography"
  },
  "code-listing": {
    "prefix": "listu",
    "body": [
      "\\newpage",
      "\\par\\noindent\\rule{\\textwidth}{0.4pt}",
      "\\lstinputlisting[language=${1|Matlab,C++,C,Python,Julia|}, caption=$2]{${0:file}}",
      "\\par\\noindent\\rule{\\textwidth}{0.4pt}\n"
    ],
    "description": "LaTeX code listing"
  },
  "contents": {
    "prefix": "contents",
    "body": [
      "\\newpage",
      "\\section{Περιεχόμενα}",
      "\\tableofcontents\n$0"
    ],
    "description": "LaTeX contents"
  },
  "LaTeX-itemize": {
    "prefix": [
      "itemu"
    ],
    "body": [
      "\\begin{itemize}",
      "\t\\item $0",
      "\\end{itemize}\n"
    ],
    "description": "LaTeX itemize"
  },
  "Enumerate": {
    "prefix": "enum",
    "body": [
      "\\\\begin{enumerate}",
      "\t\\item $0",
      "\\\\end{enumerate}"
    ],
    "description": "LaTeX enumerate"
  },
  "Description": {
    "prefix": "description",
    "body": [
      "\\\\begin{description}",
      "\t\\item[$1] $0",
      "\\\\end{description}"
    ],
    "description": "LaTeX description"
  },
  "LaTeX-equation": {
    "prefix": "eq",
    "body": [
      "\\begin{equation}\\label{eq:$1}",
      "\t$0",
      "\\end{equation}\n"
    ],
    "description": "LaTeX equation"
  },
  "cases": {
    "prefix": "cases",
    "body": [
      "\\begin{cases}",
      "\t$0 &  \\\\\\\\",
      "\t &  \\\\\\\\",
      "\\end{cases}\n"
    ],
    "description": "LaTeX cases"
  },
  "frame": {
    "prefix": "framu",
    "body": [
      "\\begin{frame}{${1:title}}",
      "\t$0",
      "\\end{frame}\n"
    ],
    "description": "LaTeX frame"
  },
  "matrix": {
    "prefix": "matrix",
    "body": [
      "\\begin{${1|matrix,pmatrix,bmatrix,Bmatrix,Vmatrix,vmatrix|}}",
      "\t$0 & \\\\\\\\",
      "\t & \\\\\\\\",
      "\\end{$1}\n"
    ],
    "description": "LaTeX matrix"
  },
  "array-LaTeX": {
    "prefix": "array",
    "body": [
      "\\begin{array}{${1:c}}",
      "\t$0\\\\\\\\",
      "\t\\\\\\\\",
      "\\end{array}\n"
    ],
    "description": "LaTeX array"
  },
  "one over": {
    "prefix": "over",
    "body": "\\frac{1}{$0}",
    "description": "special fraction"
  },
  "fraction": {
    "prefix": [
      "fr"
    ],
    "body": "\\frac{$1}{$0}",
    "description": "fraction-snippet"
  },
  "integral": {
    "prefix": "int",
    "body": "\\int_{$1}^{$2} $0 d$3",
    "description": "LaTeX integral"
  },
  "derivative": {
    "prefix": "deriv",
    "body": [
      "\\frac{d${1:f}}{d${0:x}}"
    ],
    "description": "LaTeX derivative"
  },
  "Summation-Series": {
    "prefix": "series",
    "body": [
      "\\sum_{${1:n}=${2:0}}^{${3:\\infty}} $0"
    ],
    "description": "LaTeX summation-series"
  },
  "Inline Summation": {
    "prefix": "suminline",
    "body": [
      "\\sum_{$1}^{$2}$0"
    ],
    "description": "LaTeX inline summation notation."
  },
  "parentheses": {
    "prefix": [
      "parentheses",
      "parenth"
    ],
    "body": "\\left($0\\right)",
    "description": "LaTeX parentheses"
  },
  "square_bracket": {
    "prefix": [
      "bracku",
      "square_bracket"
    ],
    "body": "\\left[$0\\right]",
    "description": "LaTeX square bracket"
  },
  "tikz-data": {
    "prefix": [
      "tizk_datu"
    ],
    "body": "\\addplot table [x =${2:collumn_name}, y =${3:collumn_name}, col sep=comma] {${1:file}};\n$0",
    "description": "tikz plot data from csv"
  },
  "siunits-declare": {
    "prefix": "sideclaru",
    "body": "\\DeclareSIUnit\\{${1:unit_call}}{${0:unit_show}}",
    "description": "siunitx declare unit"
  },
  "squareroot": {
    "prefix": "sqrt",
    "body": "\\sqrt{$0}",
    "description": "LaTeX square root"
  },
  "cos": {
    "prefix": "cos",
    "body": "\\cos($0)",
    "description": "LaTeX cosine"
  },
  "sin": {
    "prefix": "sin",
    "body": "\\sin($0)",
    "description": "LaTeX sine"
  },
  "tan": {
    "prefix": "tan",
    "body": "\\tan($0)",
    "description": "LaTeX tangent"
  },
  "log": {
    "prefix": "log",
    "body": "\\log($0)",
    "description": "LaTeX logarithm"
  },
  "ln": {
    "prefix": "ln",
    "body": "\\ln ($0)",
    "description": "LaTeX natural logarithm"
  },
  "vector": {
    "prefix": "vecu",
    "body": "\\vec{$0}",
    "description": "LaTeX vector"
  },
  "over_dot": {
    "prefix": "dotu",
    "body": "\\dot{$0}",
    "description": "LaTeX dot"
  },
  "over_ddot": {
    "prefix": "ddotu",
    "body": "\\ddot{$0}",
    "description": "LaTeX ddot"
  },
  "underline": {
    "prefix": "underu",
    "body": [
      "\\underline{$0}"
    ],
    "description": "LaTeX underline"
  },
  "newpage": {
    "prefix": "np",
    "body": "\\newpage\n$0",
    "description": "LaTeX newpage"
  },
  "cdot": {
    "prefix": "cdot",
    "body": "\\cdot$0",
    "description": "LaTeX cdot"
  },
  "proportional": {
    "prefix": [
      "analog",
      "proportional"
    ],
    "body": "\\propto$0",
    "description": "LaTeX proportional"
  },
  "infinity": {
    "prefix": [
      "infinity",
      "apeiro"
    ],
    "body": "\\infty$0",
    "description": "LaTeX infinity"
  },
  "latin": {
    "prefix": "latin",
    "body": "\\textlatin{$0}",
    "description": "LaTeX latin"
  }
}
