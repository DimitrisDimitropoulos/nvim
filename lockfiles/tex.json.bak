  "Latex-equation": {
    "prefix": ["equation", "$$", "$", "Equation", "eq"],
    "body": ["\\begin{equation}\\label{eq:$1}", "$2", "\\end{equation}", "$0"],
    "description": "An equation snippet"
  },
  "Bibliography-BOOK-style": {
    "prefix": ["book", "Book", "βιβλίο", "Βιβλίο", "BOOK"],
