      "    \\begin{tabular}[H]{||c|c|c||}",
      "        \\toprule",
      "         $1 & $2 & $3",
      "        \\\\\\\\  \\midrule \\midrule",
      "         $4 & $5 & $6",
      "        \\\\\\\\ \\midrule",
      "         $7 & $8 & $9",
      "        \\\\\\\\ \\midrule",
      "         & &",
      "        \\\\\\\\ \\midrule",
      "         & &",
      "        \\\\\\\\ \\midrule",
      "         & &",
      "        \\\\\\\\ \\midrule",
      "         & &",
      "        \\\\\\\\ \\midrule",
      "         & &",
      "        \\\\\\\\ \\midrule",
      "         & &",
      "        \\\\\\\\ \\midrule",
      "         & &",
      "        \\\\\\\\ \\midrule",
      "         & &",
      "        \\\\\\\\ \\bottomrule",
      "    \\end{tabular}",
      "    \\caption{${11:λεζάντα}}\\label{${10:label}}",
      "\\end{table}",
      ""
    ],
    "description": "A table snippet"
  },
  "Latex-Figure": {
    "prefix": ["figure", "Figure", "diagram"],
