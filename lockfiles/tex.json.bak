  "Latex-itemize": {
    "prefix": ["itemize", "item", "Item"],
    "body": [
      "\\begin{itemize}",
      "    \\item",
      "${1:text}",
      "    \\item",
      "${2:text}",
      "    \\item",
      "${3:text}",
      "\\end{itemize}"
